{
    "manifest_version": 3,
    "name": "AutoFill Extension",
    "version": "1.0",
    "description": "Automatically fill in a form on erp.iitkgp.ac.in",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://erp.iitkgp.ac.in/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}